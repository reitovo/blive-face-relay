{
  "name": "blive-face-relay",
  "license": "GPL-2.0-only",
  "dependencies": [
    "libimobiledevice",
    "ixwebsocket",
    "simdjson",
    "pkgconf",
    {
      "name": "crashpad",
      "platform": "windows"
    },
    "qttranslations",
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gui",
        "network",
        "openssl",
        "thread",
        "widgets",
        "png"
