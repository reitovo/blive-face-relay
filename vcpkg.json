    {
      "name": "qttools",
      "default-features": false,
      "features": [
        "designer",
        "linguist"
      ]
    }
  ],
  "supports": "windows | osx | linux",
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "git",
      "repository": "https://github.com/cnSchwarzer/vcpkg",
      "baseline": "4998bcb2ab2dbe987f69a194c1281d793fb6770f",
      "reference": "reito"
    }
  }
}
